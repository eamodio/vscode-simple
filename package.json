{
	"name": "simple",
	"version": "0.0.1",
	"author": {
		"name": "Eric Amodio",
		"email": "eamodio@gmail.com"
	},
	"publisher": "eamodio",
	"engines": {
		"vscode": "^1.51.0"
	},
	"displayName": "Simple",
	"description": "Simple sample",
	"categories": [
		"Other"
	],
	"homepage": "https://github.com/eamodio/vscode-simple/blob/master/README.md",
	"bugs": {
		"url": "https://github.com/eamodio/vscode-simple/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/eamodio/vscode-simple"
	},
	"contributes": {
		"commands": [
			{
				"command": "simple.setTrue",
				"title": "Set Sync Key: True",
				"category": "Simple"
			},
			{
				"command": "simple.setFalse",
				"title": "Set Sync Key: False",
				"category": "Simple"
			},
			{
				"command": "simple.setUndefined",
				"title": "Set Sync Key: Undefined",
				"category": "Simple"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "simple.view",
					"name": "Simple"
				}
			]
		},
		"colors": [
			{
				"id": "simple.iconColor1",
				"description": "Specifies the icon color 1",
				"defaults": {
					"dark": "#35B15E",
					"light": "#35B15E",
					"highContrast": "#35B15E"
				}
			},
			{
				"id": "simple.iconColor2",
				"description": "Specifies the icon color 2",
				"defaults": {
					"dark": "#B15E35",
					"light": "#B15E35",
					"highContrast": "#B15E35"
				}
			}
		]
	},
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension",
	"scripts": {
		"clean": "git clean -Xdf -e !node_modules -e !node_modules/**/*",
		"compile": "tsc -p ./",
		"lint": "eslint src/**/*.ts --fix",
		"pack": "vsce package --yarn",
		"pretty": "prettier --config .prettierrc --loglevel warn --write \"./**/*.{ts,md,json}\"",
		"reset": "yarn run clean && yarn --frozen-lockfile",
		"watch": "tsc -watch -p ./",
		"vscode:prepublish": "yarn run reset && yarn run compile"
	},
	"devDependencies": {
		"@types/node": "12.12.24",
		"@types/vscode": "1.51.0",
		"@typescript-eslint/eslint-plugin": "4.8.1",
		"@typescript-eslint/parser": "4.8.1",
		"eslint": "7.13.0",
		"eslint-cli": "1.1.1",
		"eslint-config-prettier": "6.15.0",
		"eslint-plugin-import": "2.22.1",
		"prettier": "2.1.2",
		"typescript": "4.0.5",
		"vsce": "1.81.1"
	}
}
